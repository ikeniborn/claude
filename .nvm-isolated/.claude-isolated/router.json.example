{
  "providers": {
    "deepseek": {
      "type": "deepseek",
      "apiKey": "${DEEPSEEK_API_KEY}",
      "baseURL": "https://api.deepseek.com",
      "description": "DeepSeek - Chinese LLM provider with competitive pricing"
    },
    "openrouter": {
      "type": "openrouter",
      "apiKey": "${OPENROUTER_API_KEY}",
      "baseURL": "https://openrouter.ai/api/v1",
      "description": "OpenRouter - Access to multiple models (Claude, GPT, Gemini)"
    },
    "ollama": {
      "type": "ollama",
      "baseURL": "http://localhost:11434",
      "description": "Ollama - Local model execution (no API key needed)"
    },
    "gemini": {
      "type": "gemini",
      "apiKey": "${GOOGLE_API_KEY}",
      "baseURL": "https://generativelanguage.googleapis.com/v1beta",
      "description": "Google Gemini - Direct Gemini API"
    },
    "volcengine": {
      "type": "volcengine",
      "apiKey": "${VOLCENGINE_API_KEY}",
      "baseURL": "https://ark.cn-beijing.volces.com/api/v3",
      "description": "Volcengine - ByteDance cloud AI service"
    },
    "siliconflow": {
      "type": "siliconflow",
      "apiKey": "${SILICONFLOW_API_KEY}",
      "baseURL": "https://api.siliconflow.cn/v1",
      "description": "SiliconFlow - Chinese AI cloud provider"
    }
  },
  "models": {
    "claude-sonnet-4-5": {
      "provider": "deepseek",
      "model": "deepseek-chat",
      "maxTokens": 8000,
      "temperature": 0.7,
      "description": "DeepSeek chat model (cost-effective alternative)"
    },
    "claude-opus-4-5": {
      "provider": "openrouter",
      "model": "anthropic/claude-opus-4",
      "maxTokens": 4096,
      "description": "Claude Opus 4 via OpenRouter"
    },
    "claude-sonnet-3-5": {
      "provider": "openrouter",
      "model": "anthropic/claude-3.5-sonnet",
      "maxTokens": 8192,
      "description": "Claude Sonnet 3.5 via OpenRouter"
    },
    "claude-local": {
      "provider": "ollama",
      "model": "qwen2.5-coder:latest",
      "maxTokens": 8192,
      "description": "Local Qwen 2.5 Coder model via Ollama"
    },
    "gemini-pro": {
      "provider": "gemini",
      "model": "gemini-2.0-flash-exp",
      "maxTokens": 8192,
      "description": "Google Gemini 2.0 Flash"
    }
  },
  "routing": {
    "default": "claude-sonnet-4-5",
    "rules": [
      {
        "condition": "context_length > 60000",
        "model": "claude-opus-4-5",
        "description": "Use Opus for large context"
      },
      {
        "condition": "thinking_required",
        "model": "claude-sonnet-3-5",
        "description": "Use Sonnet 3.5 for complex reasoning"
      }
    ]
  },
  "transformers": {},
  "meta": {
    "version": "1.0.0",
    "description": "Claude Code Router configuration template - Copy to router.json and configure providers",
    "documentation": "https://github.com/musistudio/claude-code-router"
  }
}
